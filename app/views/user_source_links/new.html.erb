<h1>New User Source Link</h1>

<%= form_for(@user_source_link) do |f| %>
    <% if @user_source_link.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@user_source_link.errors.count, "error") %> prohibited this user_source_link from being saved:</h2>

          <ul>
            <% @user_source_link.errors.full_messages.each do |message| %>
                <li><%= message %></li>
            <% end %>
          </ul>
        </div>
    <% end %>

    <div class="field">
      <%= f.label :'User Store Name' %><br>
      <%= f.collection_select :UserStore_id, UserStore.all, :id, :UserStoreName, include_blank:true %>
    </div>

    <div class="field">
      <%= f.label :'Hyperlink' %><br>
      <%= f.collection_select :SourceLink_id, UserStore.all, :id, :UserStoreName, include_blank:true %>
    </div>
    <div class="field">
      <%= f.label :'Created By' %><br>
      <%= f.text_field :CreatedBy %>
    </div>
    <div class="field">
      <%= f.label :'Created Date' %><br>
      <%= f.datetime_select :CreatedDate %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>

<%= link_to 'Back', user_source_links_path %>
